menuconfig BQ35100
	bool "BQ35100 lithium primary battery fuel gauge"
	depends on I2C && NEWLIB_LIBC
	help
	  Enable driver for BQ35100 BQ35100 lithium primary battery fuel gauge.

if BQ35100

config BQ35100_CALIBRATION_MODE
	bool "Calibration mode"

endif # BQ35100
